{
  "name": "@nw/localstack",
  "version": "0.3.5",
  "description": "NW Local stack",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "publishConfig": {
    "registry": "http://nexus.nw.adesa.com/repository/npm-hosted/"
  },
  "bin": {
    "localstack": "./lib/bin/localstack"
  },
  "files": [
    "lib"
  ],
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "http://github.nw.adesa.com/new-wave/nw-localstack.git"
  },
  "scripts": {
    "clean": "rimraf lib",
    "lint": "tslint --force --format verbose \"*.ts\"",
    "build": "npm run clean && npm run lint && echo Using TypeScript && tsc --version && tsc --pretty && ncp src/main/resources lib/resources && ncp src/main/javascript lib",
    "test": "mocha --compilers ts:ts-node/register test/*.spec.ts",
    "prestart": "npm run build",
    "start": "./lib/bin/localstack postgres up",
    "stop": "./lib/bin/localstack postgres down"
  },
  "dependencies": {
    "aws-sdk": "^2.54.0",
    "cli-input": "^0.2.0",
    "commander": "^2.9.0",
    "elasticdump": "^3.3.0",
    "mydockerjs": "1.14.0",
    "npm-run": "^4.1.2"
  },
  "devDependencies": {
    "@types/chai": "^3.4.35",
    "@types/node": "^7.0.18",
    "@types/sinon": "^2.2.1",
    "awesome-typescript-loader": "^3.1.2",
    "chai": "^3.5.0",
    "concurrently": "^3.4.0",
    "cucumber": "^2.0.0-rc.9",
    "mocha": "^3.2.0",
    "mocha-junit-reporter": "^1.13.0",
    "ncp": "^2.0.0",
    "nock": "^9.0.11",
    "nyc": "^10.3.0",
    "rimraf": "^2.5.4",
    "sinon": "^2.2.0",
    "ts-node": "^3.0.4",
    "tslint": "^5.2.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.2"
  },
  "peerDependencies": {},
  "nyc": {
    "lines": 80,
    "statements": 80,
    "functions": 80,
    "branches": 80,
    "include": [
      "src/main/**/*.ts"
    ],
    "exclude": [
      "typings",
      "src/test/*"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html",
      "text"
    ],
    "all": true,
    "check-coverage": true
  }
}
